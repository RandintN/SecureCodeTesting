    "build": "tsc",
    "dev": "nodemon config/med-cc.ts",
    "start": "nodemon config/med-cc.ts",
    "prod": "npm run build && npm run start"
  },
  "keywords": [
    "med",
    "medicine",
    "request",
    "chaincode",
    "smartcontract"
  ],
  "author": "fmarino, vpoliveira",
  "license": "ISC",
  "dependencies": {
    "fabric-shim": "^1.4.0",
    "node-gyp": "^3.8.0",
    "typescript": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "nodemon": "^1.18.9",
    "ts-node": "^5.0.1"
  },
  "nodemonConfig": {
    "watch": [
      ""
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts",
      "*.md",
      "*.json"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
